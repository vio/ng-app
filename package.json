		"files": [
			"test/**/*.ts",
			"!test/inputs/-util.ts",
			"!test/mocks/**"
		],
		"compileEnhancements": false,
		"extensions": [
			"ts"
		],
		"require": [
			"./test/-register.js"
		]
	}
